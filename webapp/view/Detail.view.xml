<mvc:View controllerName="com.sap.innovision.controller.Detail" xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:semantic="sap.f.semantic"
	xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc"
	xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" xmlns:layout="sap.ui.layout" xmlns:viz="sap.viz.ui5.controls"
	xmlns:viz.feeds="sap.viz.ui5.controls.common.feeds" xmlns:viz.data="sap.viz.ui5.data">
	<semantic:SemanticPage id="detail_id">
		<semantic:titleHeading>
			<Title id="title" level="H2"/>
		</semantic:titleHeading>
		<semantic:headerContent>
			
			<ObjectAttribute text="Developer: "/>
			<ObjectAttribute binding='{/suite/stat/_attributes}' text="Test suite name:{name}"/>
			
			<!--<ObjectAttribute binding='{/total/stat/0}' text="Test Category: {_text}"/>-->
			<!--   <ObjectAttribute binding='{/total/stat/0}' text="Passed: {_attributes/pass}, Failed: {_attributes/fail}"/>-->
			<!--   <ObjectAttribute binding='{/total/stat/1}' text="Test Category: {_text}"/>-->
			<!--   <ObjectAttribute binding='{/total/stat/1}' text="Passed: {_attributes/pass}, Failed: {_attributes/fail}"/>-->
			<viz:Popover id="idPopOver2"></viz:Popover>
			<viz:VizFrame id="idVizFrame" selectData="myChartClickHandler" uiConfig="{applicationSet:'fiori'}" height="300px" width="100%" vizType='donut'
				vizProperties="{plotArea : {dataLabel: { visible : true, style:{fontSize: '1rem'}} },interaction: {selectability: { mode: 'single' },legendSelection:true}}">
				<viz:dataset>
					<viz.data:FlattenedDataset data="{/Report}">
						<viz.data:dimensions>
							<viz.data:DimensionDefinition name="desc" value="{desc}"/>
						</viz.data:dimensions>
						<viz.data:measures>
							<viz.data:MeasureDefinition name="Number of test cases" value="{num}"/>
						</viz.data:measures>
					</viz.data:FlattenedDataset>
				</viz:dataset>
				<viz:feeds>
					<viz.feeds:FeedItem uid="size" type="Measure" values="Number of test cases" visible="false"/>
					<viz.feeds:FeedItem uid="color" type="Dimension" values="desc"/>
				</viz:feeds>
			</viz:VizFrame>
			<Toolbar class="sapContrast sapContrastPlus" width="100%" id="toolbar">
				<content>
					<Button text="View Log" press=".onLogClick"/>
					<ToolbarSpacer id="spacer01_1585628234749"/>
					<Button id="gb" text="Go back" visible="false" press=".onBack"/>
				</content>
			</Toolbar>
			<!--<Button text="View Log" press=".onLogClick"/>-->
		</semantic:headerContent>
		<semantic:content>
			<core:HTML id="html"></core:HTML>
		</semantic:content>
		<!--Semantic ShareMenu Buttons-->
		<semantic:sendEmailAction>
			<semantic:SendEmailAction id="shareEmail" press=".onSendEmailPress"/>
		</semantic:sendEmailAction>
		<semantic:closeAction>
			<semantic:CloseAction id="closeColumn" press=".onCloseDetailPress"/>
		</semantic:closeAction>
		<semantic:fullScreenAction>
			<semantic:FullScreenAction id="enterFullScreen"
				visible="{= !${device>/system/phone} &amp;&amp; !${app>/actionButtonsInfo/midColumn/fullScreen}}" press=".toggleFullScreen"/>
		</semantic:fullScreenAction>
		<semantic:exitFullScreenAction>
			<semantic:ExitFullScreenAction id="exitFullScreen"
				visible="{= !${device>/system/phone} &amp;&amp; ${app>/actionButtonsInfo/midColumn/fullScreen}}" press=".toggleFullScreen"/>
		</semantic:exitFullScreenAction>
	</semantic:SemanticPage>
</mvc:View>