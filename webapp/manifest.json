{
	"_version": "1.12.0",
	"sap.app": {
		"id": "com.sap.innovision",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"resources": "resources.json",
		"ach": "ach",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "com.sap.innovision.view.app",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.sap.innovision.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "com.sap.innovision.view",
				"clearTarget": false,
				"controlId": "myapp_fullscreen",
				"controlAggregation": "pages"
			},
			"routes": [
				{
					"name": "Master_r",
					"pattern": "",
					"target": [
						"FullScreen_t"
					]
				},
				{
					"name": "Detail_r",
					"pattern": "{botid} {sysid}",
					"target": [
						"Master_t", "Detail_t"
					]
				}
			],
			"targets": {
				"Master_t": {
					"viewType": "XML",
					"viewName": "Master",
					"viewLevel": 1,
					"viewId": "master_id",
					"controlId": "splitapp",
					"controlAggregation": "masterPages"
				},
				"Detail_t": {
					"viewType": "XML",
					"viewName": "Detail",
					"viewLevel": 1,
					"viewId": "detail_id",
					"controlId": "splitapp",
					"controlAggregation": "detailPages"
				},
				"FullScreen_t":{
					"viewType": "XML",
					"viewName": "Master",
					"viewLevel": 1,
					"viewId": "master_id"
				}
			}
		}
	}
}